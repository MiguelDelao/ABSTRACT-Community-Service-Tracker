<Window x:Class="MDZFBLACommunityService.AdminHub"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MDZFBLACommunityService"
        mc:Ignorable="d"
             TextElement.Foreground="{DynamicResource MaterialDesignBody}"
     TextElement.FontWeight="Regular"
     TextElement.FontSize="13"
     TextOptions.TextFormattingMode="Ideal"
     TextOptions.TextRenderingMode="Auto"
     Background="{DynamicResource MaterialDesignPaper}"
     FontFamily="{DynamicResource MaterialDesignFont}"
        Title="AdminHub" Height="675" Width="1200" WindowStartupLocation="CenterScreen">

    <Grid Background="#FF1B262C">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>



        </Grid.ColumnDefinitions>
        <Rectangle Fill="#FF0F4C75" HorizontalAlignment="Left" Height="44" Margin="10,352,0,0" VerticalAlignment="Top" Width="232"/>

        <Menu HorizontalAlignment="Left" Height="31" VerticalAlignment="Top" Width="133"  Background="#FF0F4C75" />
        <Rectangle MouseDown="Rectangle_MouseDown" Fill="#FF0F4C75" HorizontalAlignment="Left" Height="31" Margin="117,0,0,0" VerticalAlignment="Top" Width="1075"/>

        <Button Content="Button" HorizontalAlignment="Left" Margin="1152,0,0,0" VerticalAlignment="Top" Width="40" Height="31" BorderThickness="0" UseLayoutRounding="False" />
        <Button Content="Button" HorizontalAlignment="Left" Height="44" Margin="17,144,0,0" VerticalAlignment="Top" Width="210" UseLayoutRounding="True" Background="#FFBBE1FA" Foreground="#DD000000"/>
        <Rectangle Fill="#FF0F4C75" HorizontalAlignment="Left" Height="56" Margin="0,83,0,0" VerticalAlignment="Top" Width="247"/>
        <Button x:Name="asd" Content="Button" HorizontalAlignment="Left" Height="44" Margin="0,144,0,0" VerticalAlignment="Top" Width="247" UseLayoutRounding="True" Background="#FFBBE1FA" Foreground="#DD000000" BorderBrush="{x:Null}" Click="Asd_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="44" Margin="0,242,0,0" VerticalAlignment="Top" Width="247" UseLayoutRounding="True" Background="#FFBBE1FA" Foreground="#DD000000" BorderBrush="{x:Null}"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="44" Margin="0,193,0,0" VerticalAlignment="Top" Width="247" UseLayoutRounding="True" Background="#FFBBE1FA" Foreground="#DD000000" BorderBrush="{x:Null}"/>
        <Label Content="Student View" HorizontalAlignment="Left" Height="30" Margin="56,96,0,0" VerticalAlignment="Top" Width="134" FontSize="20"/>
        <Rectangle Fill="#FF0F4C75" HorizontalAlignment="Left" Height="56" Margin="0,291,0,0" VerticalAlignment="Top" Width="247" RadiusY="3.5" RadiusX="3.5"/>
        <Button x:Name="asd_Copy" Content="Button" Height="44" Margin="10,352,950,0" VerticalAlignment="Top" UseLayoutRounding="True" Background="{x:Null}" Foreground="#DD000000" BorderBrush="{x:Null}" Click="Asd_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="44" Margin="10,450,0,0" VerticalAlignment="Top" Width="232" UseLayoutRounding="True" Background="#FFBBE1FA" Foreground="#DD000000" BorderBrush="{x:Null}"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="44" Margin="10,401,0,0" VerticalAlignment="Top" Width="232" UseLayoutRounding="True" Background="#FFBBE1FA" Foreground="#DD000000" BorderBrush="{x:Null}"/>
        <Label Content="Student View" HorizontalAlignment="Left" Height="30" Margin="56,303,0,0" VerticalAlignment="Top" Width="134" FontSize="20"/>
        <Frame NavigationUIVisibility="Hidden" x:Name="MainFrame" Content="Frame" HorizontalAlignment="Left" Height="613" Margin="247,31,0,0" VerticalAlignment="Top" Width="945"/>

    </Grid>

</Window>
